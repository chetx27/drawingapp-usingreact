{
  "name": "advanced-react-drawing-app",
  "version": "2.0.0",
  "description": "A professional-grade, feature-rich drawing application built with React and TypeScript",
  "homepage": "https://chetx27.github.io/drawingapp-usingreact",
  "bugs": {
    "url": "https://github.com/chetx27/drawingapp-usingreact/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chetx27/drawingapp-usingreact.git"
  },
  "license": "MIT",
  "author": {
    "name": "Chetana G",
    "url": "https://github.com/chetx27"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "keywords": [
    "react",
    "typescript",
    "drawing-app",
    "canvas",
    "graphics",
    "paint",
    "sketch",
    "art",
    "digital-art",
    "web-app"
  ]
}